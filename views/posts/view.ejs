<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <title>Document</title>
</head>
<body>
  <h1>name's posts</h1>
  <table>
    <thead>
      <tr>
        <th>img</th>
        <th>Post Title</th>
        <th>content</th>
      </tr>
    </thead>
    <tbody>
      <% posts.forEach( post => { %>
        <tr>
          <td><img src="<%= post.img %>" alt=""></td>
          <td><%= post.title %></td>
          <td><%= post.content %></td>
        </tr>
      <% }) %>
    </tbody>
  </table>
</body>
</html>
