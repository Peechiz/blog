<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <title>Rocket</title>
</head>
<body>
  <h1>Users</h1>
  <table>
    <thead>
      <tr>
        <th>id</th>
        <th>Avatar</th>
        <th>User Name</th>
      </tr>
    </thead>
    <tbody>
      <% users.forEach(function(user){ %>
        <tr>
          <td><%= user.id %></td>
          <td><img src="<%= user.avatar %>"></td>
          <td><a href="users/<%= user.id %>"><%= user.user %></a></td>
        </tr>
      <% }) %>
    </tbody>
  </table>
</body>
</html>
